<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/movieListFragment">

    <fragment
        android:id="@+id/movieListFragment"
        android:name="com.devsu.movie_ui_tv.fragments.MovieListFragment"
        android:label="MovieListFragment">


        <action
            android:id="@+id/action_movieListFragment_to_errorFragment"
            app:destination="@id/errorFragment" />
    </fragment>
    <fragment
        android:id="@+id/errorFragment"
        android:name="com.devsu.movie_ui_tv.fragments.ErrorFragment"
        android:label="ErrorFragment" />

    <dialog
        android:id="@+id/spinnerFragment"
        android:name="com.devsu.movie_ui_tv.fragments.SpinnerFragment"
        android:label="SpinnerFragment" />
</navigation>